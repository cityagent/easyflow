<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lecoder.easyflow.modules.flowlong.mapper.DepartmentMapper">


    <select id="selectList" resultType="org.lecoder.easyflow.modules.flowlong.dto.DepartmentDto">
        select * from wx_department department
        <where>
            <if test="name != null and name != ''">
                and department.name like concat('%',#{name},'%')
            </if>
        </where>

    </select>
</mapper>
